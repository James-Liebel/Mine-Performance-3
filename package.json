{
  "name": "mine-performance-academy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prepush": "npm run lint && npm run typecheck && npm run test -- --run && npm run build",
    "security:check": "npm run security:audit",
    "security:audit": "npm audit --audit-level=high",
    "security:audit:ci": "npm audit --audit-level=critical",
    "crawl": "npx tsx scripts/crawl-statstak.ts",
    "explore": "npx tsx scripts/explore.ts",
    "extract-leaderboard": "npx tsx scripts/extract-leaderboard.ts",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "next": "14.2.35",
    "next-auth": "^4.24.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.14.1",
    "stripe": "^17.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "playwright": "^1.49.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
